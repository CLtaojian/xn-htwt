<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cdkj.loan.dao.ISYSRoleNodeDao">

	<insert id="insert_sysRoleNode" parameterType="sysRoleNode">
		INSERT INTO tsys_role_node(
		node_code,
		role_code
		)
		VALUES(
		#{node_code},
		#{role_code}
		)
	</insert>

</mapper>
